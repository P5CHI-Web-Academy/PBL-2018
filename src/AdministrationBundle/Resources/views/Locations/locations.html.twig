{% extends 'base.html.twig' %}

{% block title %}Locations index{% endblock %}
{% block stylesheets %}{% endblock %}

{% block body %}

    <br/>

    {% for location in locations %}
        {{ location.location }}

        <a href="{{ path('location_delete', {'id': location.id}) }}">Delete</a>

        {% for tag in location.tags %}
            {{ tag.tag }}
        {% endfor %}

        <br/>
    {% endfor %}


{% endblock %}

{% block javascripts %}{% endblock %}